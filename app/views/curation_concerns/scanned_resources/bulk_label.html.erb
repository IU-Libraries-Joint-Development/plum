<%= link_to "Back to Parent", polymorphic_path([main_app, :curation_concerns, @presenter]) %>
<div data-action="bulk-label">
  <%= render "bulk_label_actions" %>
  <ul class="col-xs-8">
    <% @members.each do |member| %>
      <li>
        <span class="text">
          <%= member.to_s %>
        </span>
        <%= simple_form_for [main_app, :curation_concerns, member], remote: true do |f| %>
          <%= f.input :title, as: :hidden, input_html: { name: "file_set[title][]" } %>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>
